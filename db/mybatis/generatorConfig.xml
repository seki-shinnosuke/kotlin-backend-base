<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
    <context id="MySQLTables2" targetRuntime="MyBatis3Kotlin">
        <commentGenerator>
            <property name="suppressDate" value="true" />
        </commentGenerator>
        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:23306/base?allowPublicKeyRetrieval=true&amp;useSSL=false"
                userId="base_user"
                password="base_pass" />
        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>
        <!-- モデルクラスを生成する設定 -->
        <javaModelGenerator
                targetPackage="seki.kotlin.backend.base.common.db.model"
                targetProject="common/src/main/kotlin">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- マッパークラスを生成する設定 -->
        <javaClientGenerator
                targetPackage="seki.kotlin.backend.base.common.db.mapper"
                targetProject="common/src/main/kotlin"
                type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <!-- コードを生成するテーブルを指定 -->
        <!-- Master table -->
        <!-- Transaction table -->
        <table tableName="client_user">
            <generatedKey column="client_user_id" sqlStatement="MySql" identity="true" />
        </table>
        <table tableName="client_user_password" />
        <table tableName="access_log_management">
            <generatedKey column="access_log_id" sqlStatement="MySql" identity="true" />
        </table>
        <table tableName="email_send">
            <generatedKey column="email_send_id" sqlStatement="MySql" identity="true" />
        </table>
        <!-- System table -->
        <table tableName="batch_start_control" />
        <table tableName="virtual_datetime_management" />
    </context>
</generatorConfiguration>